{
    "name": "gregor/jus",
    "description": "Based on the UX theme",
    "minimum-stability": "stable",
    "type": "wordpress-theme",
    "require": {
        "squizlabs/php_codesniffer": "3.6.*",
        "php": ">=7.4.30"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Gregor\\Jus\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Greg Miller",
            "email": "hello@gregmiller.io"
        }
    ],
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}

